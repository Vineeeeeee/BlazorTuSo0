@page "/compute"
@inherits ComputeBase 
@rendermode InteractiveServer 

<h3>Calculator</h3>

<div>
    <input type="number" @bind="Num1" />
    <input type="number" @bind="Num2" />
    <button @onclick="Calculate">Calculate</button>
    <button @onclick="Clear">Clear</button>
</div>

<p>Result: @Num</p>


